  ],
  "dependencies": {
    "@google/genai": "^1.29.0",
    "@google/generative-ai": "^0.24.1",
    "dotenv": "^17.2.3",
    "ink": "^6.4.0",
    "ink-big-text": "^2.0.0",
    "ink-gradient": "^3.0.0",
    "ink-select-input": "^6.2.0",
    "ink-spinner": "^5.0.0",
    "meow": "^11.0.0",
    "react": "^19.2.0",
    "react-error-boundary": "^6.0.0"
  },
  "devDependencies": {
    "@types/react": "^19.2.2",
    "ava": "^5.2.0",
    "chalk": "^5.2.0",
    "ink-testing-library": "^3.0.0",
    "prettier": "^2.8.7",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.3"
  },
  "ava": {
    "extensions": {
      "ts": "module",
      "tsx": "module"
